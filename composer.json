{
    "name": "sobanvuex/php-newrelic",
    "description": "PHP Library for New Relic Agent",
    "keywords": ["newrelic", "new relic", "monitoring", "performance", "apm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Soban",
            "email": "me@soban.co",
            "homepage": "https://soban.co",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "me@soban.co",
        "issues": "https://github.com/SobanVuex/php-newrelic/issues",
        "source": "https://github.com/SobanVuex/php-newrelic"
    },
    "autoload": {
        "psr-4": {
            "SobanVuex\\NewRelic\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "pimple/pimple": "~3",
        "apigen/apigen": "~4",
        "codacy/coverage": "dev-master"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "suggest": {
        "pimple/pimple": "Pimple is a small dependency injection container for PHP",
        "ext-newrelic": "Get the New Relic PHP extension from http://newrelic.com"
    }
}
